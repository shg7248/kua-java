
public class Ex04_07_정렬 {
	public static void main(String[] args) {

		int[] arr = {8, 3, 5, 2, 9};
		
		for(int i : arr) {
			System.out.print(i+"\t");
		}
		
		System.out.println();
		
		for(int i=0;i<arr.length;i++) {
			for(int j=i+1;j<arr.length;j++) {
				if(arr[i] > arr[j]) {
					int temp = arr[i];
					arr[i] = arr[j];
					arr[j] = temp;
				}	
			}
		}
		
		for(int i : arr) {
			System.out.print(i+"\t");
		}
		
		System.out.println();
	}
}
