import java.util.Scanner;

public class Ex03_04_switch_case {
	public static void main(String[] args) {
		int n = 2;
		if(n==1) {
			System.out.println("1입니다.");
		}
	}
}

/*
 * 		switch(정수 or 정수형변수 or 문자열){
 * 			case 정수1 : 실행1;
 * 				break;
 * 			case 정수2 : 실행2;
 * 				break;
 * 			case 정수n : 실행n;
 * 				break;
 * 			default : 실행3;
 * 				break;
 * 		}
 * 
 */
