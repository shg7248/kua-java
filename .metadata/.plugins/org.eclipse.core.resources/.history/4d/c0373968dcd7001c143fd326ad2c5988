import java.util.Arrays;

public class Ex04_05_2차원 {
	public static void main(String[] args) {

		int[][] arr = new int[][] 
			{
				{80,72,63,83}, 
				{92,21,89,29},
				{99,49,59,92}
			};
			
			System.out.println();
			
			int[] arr2 = {5,4,7,3,2,1,9};
			Arrays.sort(arr2);
			System.out.println(Arrays.binarySearch(arr2, 4));
		
	}
}
