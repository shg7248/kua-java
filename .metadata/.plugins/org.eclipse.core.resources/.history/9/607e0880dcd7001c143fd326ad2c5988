import java.util.Arrays;

public class Ex04_05_2차원 {
	public static void main(String[] args) {

		int[][] arr = new int[][] 
			{
				{80,72,63,83}, 
				{92,21,89,29},
				{99,49,59,92}
			};
			
			System.out.println();
			
			String[] arr2 = {"송","학","관","인","천","살","아"};
			Arrays.sort(arr2);
			System.out.println(Arrays.binarySearch(arr2, "송"));
		
	}
}
