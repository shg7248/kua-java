import java.util.Scanner;

public class Ex02_06_ifelse {
	public static void main(String[] args) {
	
		Scanner sc = new Scanner(System.in);
		
		int a, b;
		
		System.out.print("숫자1 입력 => ");
		a = sc.nextInt();
		
		System.out.print("숫자2 입력 => ");
		b = sc.nextInt();
		
		System.out.println("a = "+a+", b = "+b);
	}
}
