
class Student {
	
	int kor;
	int eng;
	int math;
	SongCalculator cal;
	
	Student(SongCalculator cal) {
		this.cal = cal;
	}
	
	public int add() {
		return cal.add(kor, eng, math);
	}
	
	public int sub() {
		
	}
	
}

class SongCalculator {
	public int add(int a, int b, int c) {
		return a + b;
	}
}

public class Ex05_05_메서드 {
	public static void main(String[] args) throws Exception { // 정의
		
		Student s = new Student(new SongCalculator());
		System.out.println(s.add());
		
	}
}