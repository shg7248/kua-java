class Student {
	
	int kor;
	int eng;
	int math;
	Calculator cal;
	
	Student(Calculator cal) {
		this.cal = cal;
	}
	
	public void calc() {
		
	}
	
}

class Calculator {
	public int add(int a, int b) {
		return a + b;
	}
}

