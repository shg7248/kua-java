import java.util.Scanner;

public class Ex02_06_ifelse {
	public static void main(String[] args) {
	
		Scanner sc = new Scanner(System.in);
		
		int a, b;
		
		System.out.print("a => ");
		a = sc.nextInt();
		
		System.out.print("b => ");
		b = sc.nextInt();
		
		System.out.println("a="+a+", b="+b);
	}
}
