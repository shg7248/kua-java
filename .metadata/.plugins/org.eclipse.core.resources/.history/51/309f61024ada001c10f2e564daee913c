import java.util.Scanner;

class Student {
	
	int kor;
	int eng;
	int math;
	
}

public class Ex05_05_메서드 {
	public static void main(String[] args) throws Exception { // 정의
		
		int kor, eng, m;
		
		Student s = null;
		s = new Student();
		
		Scanner sc = new Scanner(System.in);
		
		System.out.print("국어점수 : ");
		kor = sc.nextInt();
		System.out.print("영어점수 : ");
		eng = sc.nextInt();
		System.out.print("수학점수 : ");
		m = sc.nextInt();
	}
}