package com.cos.reflect.controller;

import com.cos.reflect.anno.RequestMapping;

public class UserController {
	
	@RequestMapping("/join")
	public String join() { // username, password, email
		return "/";
	}
	
	@RequestMapping("/login")
	public String login() { // username, password
		return "/";
	}
	
	@RequestMapping("/user")
	public String user() {
		return "/";
	}
	
	@RequestMapping("/hello")
	public String hello() {
		return "/";
	}
}
