
class Snack {
	String company;
	String name;
	int price;
}

public class Ex05_02_클래스 {
	public static void main(String[] args) {
		Snack s1 = new Snack();
		s1.company = "농심";
		s1.name = "새우깡";
		s1.price = 1000;
		
		System.out.println(s1.company);
		System.out.println(s1.name);
		System.out.println(s1.price);
		
		Snack s = new Snack();
		s.company = "농심";
		s.name = "새우깡";
		s.price = 1000;
		
		System.out.println(s.company);
		System.out.println(s.name);
		System.out.println(s.price);
	}
	}
}
