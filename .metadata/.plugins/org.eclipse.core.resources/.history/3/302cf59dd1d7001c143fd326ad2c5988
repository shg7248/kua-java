
public class Ex03_15_송학관 {
	public static void main(String[] args) {
		
		int i,j;
		for(i=2;i<=9;i++) {
			if(i%2!=0)
				continue;
			System.out.println("***"+i+"단***");
			for(j=1;j<=9, j>i;j++) {
				if(j>i)
					break;
				System.out.println(i+"*"+j+"="+i*j);
			}
			System.out.println("===========");
		}
		
	}
}
