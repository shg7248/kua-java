
class Snack {
	String company;
	String name;
	int price;
}

public class Ex05_02_클래스 {
	public static void main(String[] args) {
		Snack s1 = new Snack();
		s1.company = "농심";
		s1.name = "새우깡";
		s1.price = 1000;
		
		System.out.println(s1.company);
		System.out.println(s1.name);
		System.out.println(s1.price);
		
		System.out.println();
		
		Snack s2 = new Snack();
		s2.company = "농심";
		s2.name = "브라보콘";
		s2.price = 2000;
		
		System.out.println(s2.company);
		System.out.println(s2.name);
		System.out.println(s2.price);
	}
}
