
public class Ex05_06_배열넘기기 {
	public static void main(String[] args) throws Exception { // 정의
		
		int a=1, b=2, c=3;
		
		sub(a, b, c);
		
		Ex05_06_배열넘기기 e = new Ex05_06_배열넘기기();
		e.sub2();
		
	}
	
	static void sub(int ...z) {
		System.out.println("sub 메서드");
		System.out.println(z);
		// return;
	}
	
	void sub2() {
		System.out.println("sub 메서드");
	}
}