import java.util.Scanner;

public class Ex05_10_송학관 {
	public static void main(String[] args) throws Exception { // 정의
	
		Scanner sc = new Scanner(System.in);
		// 정수 2개 입력
		int a, b;
		a = sc.nextInt();
		b = sc.nextInt();
		
		// 3 4 : 3의 4승
		int result = pow(3, 4);
		System.out.println(result);
	}
	
	static int pow(int a, int b) {
		if(b == 0) return 1;
		return a * pow(a, b--);
	}
}