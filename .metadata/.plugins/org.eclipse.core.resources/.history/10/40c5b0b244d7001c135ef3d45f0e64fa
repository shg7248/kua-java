
public class Ex04_02_1차원 {
	public static void main(String[] args) {
		
		int[] score = {79,88,99,33,100,55,95};
		
		System.out.println("length : "+score.length);
		
		int i;
		for(i=0;i<score.length;i++)
			System.out.print(score[i]+" ");
		
		System.out.println();
		
		for(int j:score)
			System.out.print(j+" ");
		
		System.out.println();
	}
}
