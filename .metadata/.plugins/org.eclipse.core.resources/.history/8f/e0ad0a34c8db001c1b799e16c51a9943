
class Sports {
	String name;
	int inwon;
	
	Sports() {
		name = "야구";
		inwon = 9;
	}
	
	Sports(String name, int inwon) {
		this.name = name;
		this.inwon = inwon;
	}
}

public class Ex06_01_상속 {
	public static void main(String[] args) {
		
		Sports s1 = new Sports();	// 야구, 9
		Sports s2 = new Sports("축구", 11);
		
	}
}
