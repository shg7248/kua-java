
public class Ex03_13_break_continue {
	public static void main(String[] args) {
		
		int i;
		for(i=1;i<=10;i++) {
			if(i==5) {
				break; // 반복문을 빠져나간다.
			}
			System.out.print(i+" ");
		}
		
		System.out.println();
		
		i=0;
		for(i=1;i<=10;i++) {
			if(i==5) {
				continue; // 
			}
			System.out.print(i+" ");
		}
	}
}
