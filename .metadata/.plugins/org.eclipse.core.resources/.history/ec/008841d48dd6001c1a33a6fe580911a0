
public class Ex03_07_송학관3 {
	public static void main(String[] args) {
		
		int i;
		int sum1=0;
		for(i=1;i<=100;i++) {
			sum1+=i;
			if(i%10==0) {
				System.out.println("1~"+i+"="+sum1);
			}
		}
		
		int j;
		for(j=1;j<=100;j++) {
			int sum2=0;
			sum2+=j;
			if(j%10==0) {
				System.out.println("1~"+j+"="+sum2);
			}
		}
			
	}
}
