
public class Ex05_03_메서드 {
	public static void main(String[] args) { // main 메서드 정의
		
		show(); // show 메서드 호출
		
	}
	
	static void show() { // show 메서드 정의
		System.out.println("show 메서드 정의 부분");
		System.out.println("즐거운 자바 공부");
	}
}

/*
 * 		아침기상
 * 		세수
 * 		냉장고에서 반찬꺼내기
 * 		밥솥 밥 담기
 * 		숟가락 놓기
 * 		설겆이
 * 		식탁 닦기
 * 		학원
 * 		공부
 * 		
