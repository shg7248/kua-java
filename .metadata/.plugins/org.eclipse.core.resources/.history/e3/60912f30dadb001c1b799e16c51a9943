
class Person {
	String name;
	int age;
	String addr;
	
	Person() {
		super();
	}
	
	Person(String name, int age, String addr) {
		super();
		this.name = name;
		this.age = age;
		this.addr = addr;
	}
}

class Employee extends Person {

	String company;
	String title;
	
	Employee(String name, int age, String addr, String company, String title) {
		super(name, age, addr);
		this.company = company;
		this.title = title;
	}
} // Employee

class Student extends Person {

	String school;
	int kor;
	
	Student() {
		super();
	}
} // Student

