class Calc {
	public static int max(int a, int b) {
		return a > b ? a : b;
	}
}

public class Ex05_11_Math {
	public static void main(String[] args) throws Exception { // 정의

		int a = 130, b = 40;
		// int result = max(a, b);
		
		// 자바를 만든 사람들이 Math 클래스를 만들어 놓았음
		int result = Math.max(a, b);
		System.out.println("result : " + result);
		
	}
}